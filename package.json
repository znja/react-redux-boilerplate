{
  "name": "react-es6-sass-boilerplate",
  "version": "1.0.0",
  "description": "A React, React-Redux, React-Router, and Sass boilerplate using webpack",
  "main": "index.js",
  "repository": "git@github.com:znja/React-Sass-Boilerplate.git",
  "scripts": {
    "start": "node server.js --env=dev",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy & webpack --env=dist",
    "copy": "cp ./src/index.html ./src/favicon.ico ./dist",
    "clean": "rm -rf dist/*"
  },
  "keywords": [
    "react",
    "es6",
    "sass",
    "webpack",
    "boilerplate",
    "hot",
    "reload"
  ],
  "author": "Znja <carloscorvaia@gmail.com> (http://carloscorvaia.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "lodash": "^4.10.0",
    "node-sass": "^3.4.2",
    "open": "0.0.5",
    "react": "^15.0.1",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.1.0",
    "redux": "^3.4.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7"
  }
}
